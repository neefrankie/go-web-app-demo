{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "../build",
    "target": "es5",
    "module": "es6",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "include": [
    "script/*"
  ]
}